.tabbar-box {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
  z-index: 999;
}

.tabbar-navigator {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  height: 100%;
  position: relative;
}

.tabbar-icon {
  width: 56rpx;
  height: 56rpx;
}

.special-wrapper {
  position: absolute;
  left: 77rpx;
  top: -46rpx;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  border: 0rpx solid red;
  box-sizing: border-box;
  text-align: center;
  padding: 0rpx;
}

.special-wrapper .tabbar-icon {
  width: 92rpx;
  height: 92rpx;
}
.special-text-wrapper{
  width: 56rpx;
  height: 56rpx;
}
.tabbar-text {
  font-size: 20rpx;
 
}
